{
  "name": "meetups-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.7",
    "@types/react": "^18.0.26",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.9",
    "axios": "^1.2.2",
    "classnames": "^2.3.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "formik": "^2.2.9",
    "i18next": "^22.4.9",
    "mobx": "^6.7.0",
    "mobx-persist-store": "^1.1.3",
    "mobx-react-lite": "^3.4.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-i18next": "^12.1.4",
    "react-router-dom": "^6.5.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.0",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/node-logger": "^6.5.14",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "0.0.13",
    "babel-plugin-named-exports-order": "0.0.2",
    "css-loader": "^6.7.3",
    "cypress": "^12.0.2",
    "husky": "^8.0.2",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "prop-types": "^15.8.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "storybook-addon-react-router-v6": "^0.2.1",
    "storybook-react-i18next": "^1.1.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext ts,tsx src",
    "lint:fix": "eslint --fix --ext ts,tsx src",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit ",
    "test:e2e": "cypress open",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "cd .. && husky install client/.husky"
  },
  "lint-staged": {
    "**/*": "npx prettier --write --ignore-unknown",
    "*.{ts,tsx}": "npx eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react-hooks/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "coverageReporters": [
      "html",
      "text",
      "text-summary",
      "cobertura"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
